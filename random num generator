
// Java program for rand nums 1-6
import java.util.*; 
import java.util.Random;

public class Dice 
{ 
    public static void main(String args[]) 
    { 
        
        //create a rand class
        Random randnum = new Random();

        //create a varaible for rand num with range 0f 1-6 
        int randDice = 1 + randnum.nextInt(6);

    
        System.out.println("Random num is: " + randDice); 
    } 
} 
